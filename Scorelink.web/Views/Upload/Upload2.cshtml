
@{
    ViewBag.Title = "Upload 2";
}
<link href="~/plugins/jquery-upload-file-master/css/uploadfile.css" rel="stylesheet" />
<div class="card card-primary">
    <div class="card-header">
        <h3 class="card-title">Upload PDF file for scan</h3>
    </div>
    <!-- /.card-header -->
    <!-- form start -->
    <form role="form">
        <div class="card-body">
            <div class="form-group">
                <div class="input-group">
                    <div id="fileuploader">Upload</div>
                </div>
            </div>
        </div>
        <!-- /.card-body -->
    </form>
</div>
@section scripts{
    <!-- bs-custom-file-input -->
    <script src="~/plugins/jquery-upload-file-master/js/jquery.uploadfile.min.js"></script>
    <script src="~/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>
    <script src="~/Scripts/Support.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            bsCustomFileInput.init();
        });
    </script>
    <script src="~/Scripts/Pages/UploadMaster.js"></script>
}